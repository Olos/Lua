<?xml version="1.1" ?>
<settings>
	<global>
		<blacklist />					<!-- When in blacklist mode, it will join all invites except from these people. Not related to the ingame blacklist feature. -->
		<mode>whitelist</mode>			<!-- One of two modes: whitelist or blacklist mode. -->
		<autodecline>true</autodecline>	<!-- If set to true, will automatically decline invites from people on your blacklist. -->
		<whitelist />					<!-- When in whitelist mode, it will only join invites from these people. -->
	</global>
</settings>
